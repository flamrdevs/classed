{
  "name": "@classed/solid",
  "version": "0.2.0",
  "description": "Classed components for Solid",
  "keywords": [
    "class",
    "classname",
    "utility",
    "typescript",
    "solid",
    "component"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
