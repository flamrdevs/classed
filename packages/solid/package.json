  "scripts": {
    "clean": "rimraf dist",
    "build": "vite build",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flamrdevs/classed.git"
  },
  "author": "flamrdevs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flamrdevs/classed/issues"
  },
  "homepage": "https://classed.pages.dev",
  "dependencies": {
    "@classed/utils": "0.2.0"
  },
  "peerDependencies": {
    "solid-js": "1.8.0 - 2.0.0"
  },
  "devDependencies": {
    "solid-js": "^1.8.16"
  }
}
