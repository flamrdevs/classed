{
  "name": "@classed/react",
  "version": "0.2.0",
  "description": "Classed components for React",
  "keywords": [
    "class",
    "classname",
    "utility",
    "typescript",
    "react",
    "component"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "vite build",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flamrdevs/classed.git"
  },
  "author": "flamrdevs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flamrdevs/classed/issues"
  },
  "homepage": "https://classed.pages.dev",
  "dependencies": {
    "@classed/utils": "0.2.0"
  },
  "peerDependencies": {
    "react": "18.2.0 - 19.0.0",
    "react-dom": "18.2.0 - 19.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
